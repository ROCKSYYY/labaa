#include <iostream>
#include <cmath>
#include <iomanip>
#include <locale>
using namespace std;

int main() {
    setlocale(LC_ALL, "RU");
    double R = 1530.0;
    double C = 0.81e-7;
    const double PI = 3.141592653589793;

    // значения L (Гн)
    double Lvals[] = { 0.05, 0.06, 0.07, 0.08, 0.09, 0.10, 0.20, 0.30, 0.40, 0.50 };
    int n = sizeof(Lvals) / sizeof(Lvals[0]);

    cout << fixed << setprecision(3);
    cout << "   L (Гн)    T (мс)" << endl;

    for (int i = 0; i < n; i++) {
        double L = Lvals[i];

        double term = 1.0 / (L * C) - pow(R / (2.0 * L), 2);

        if (term <= 0.0) {
            cout << setw(8) << L << "    " << "---" << endl;
        }
        else {
            double omega = sqrt(term);
            double T = 2000.0 * PI / omega;
            cout << setw(8) << L << "    " << T << endl;
        }
    }

    return 0;
}
